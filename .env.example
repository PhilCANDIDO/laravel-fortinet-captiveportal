APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=sqlite
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=laravel
# DB_USERNAME=root
# DB_PASSWORD=

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Prevent concurrent admin sessions (set to false to disable)
PREVENT_CONCURRENT_SESSIONS=true

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
QUEUE_CONNECTION=database

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"

# FortiGate API Configuration
FORTIGATE_API_URL=https://192.168.1.1/api/v2
FORTIGATE_API_TOKEN=your_api_token_here
FORTIGATE_VERIFY_SSL=false
FORTIGATE_TIMEOUT=30
FORTIGATE_USER_GROUP=captive_portal_users

# FortiGate Session Settings
FORTIGATE_SESSION_TIMEOUT=86400
FORTIGATE_GUEST_SESSION_TIMEOUT=86400
FORTIGATE_CONSULTANT_SESSION_TIMEOUT=2592000

# FortiGate Retry Configuration
FORTIGATE_RETRY_MAX_ATTEMPTS=3
FORTIGATE_RETRY_INITIAL_DELAY=1000
FORTIGATE_RETRY_MAX_DELAY=10000
FORTIGATE_RETRY_MULTIPLIER=2

# FortiGate Circuit Breaker
FORTIGATE_CB_FAILURE_THRESHOLD=5
FORTIGATE_CB_RECOVERY_TIME=60
FORTIGATE_CB_SUCCESS_THRESHOLD=2

# FortiGate Cache
FORTIGATE_CACHE_ENABLED=true
FORTIGATE_CACHE_TTL=300
FORTIGATE_CACHE_PREFIX=fortigate

# FortiGate Logging
FORTIGATE_LOGGING_ENABLED=true
FORTIGATE_LOG_CHANNEL=stack
FORTIGATE_LOG_REQUESTS=true
FORTIGATE_LOG_RESPONSES=false

# FortiGate Monitoring
FORTIGATE_MONITORING_ENABLED=true
FORTIGATE_METRICS_PREFIX=fortigate

# Audit Settings
AUDIT_RETENTION_DAYS=90
